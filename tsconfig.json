{
	"compilerOptions": {
		"removeComments": true,
		"preserveConstEnums": true,
		"strictFunctionTypes": false,
		"sourceMap": false,
		"allowJs": true,
		"incremental": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"outDir": "./dist",
		"resolveJsonModule": true
	},
	"include": ["./src/**/*"],
	"exclude": ["./node_modules/**/*"]
}
